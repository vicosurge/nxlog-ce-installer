$success = TRUE;
if 2010-01-01 00:00:00 > now() $success = FALSE;
if 2010-01-01 00:00:00 >= now() $success = FALSE;
if 2010-01-01 00:00:00 < 2010-01-01 00:00:00 $success = FALSE;
if 2010-01-01 00:00:01 <= 2010-01-01 00:00:00 $success = FALSE;
if 2010-01-01 00:00:00 != 2010-01-01 00:00:00 $success = FALSE;
if not (2010-01-01 00:00:00 == 2010-01-01 00:00:00) $success = FALSE;
